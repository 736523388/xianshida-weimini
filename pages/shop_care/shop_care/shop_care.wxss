page{
  background:#f1f1f1;
}

.allselect {
  width:25%;
  height: 100%;
}

.allselect>view {
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.allselect>view {
  margin-left: 20rpx;
}

.gou {
  width: 35rpx;
  height: 35rpx;
  margin-right: 21rpx;
}

.allselect>view>text {
  color: #121314;
  font-weight: 600;
  font-size: 28rpx;
  margin-left: 10rpx;
}

.total-select {
  width: 45rpx;
  height: 45rpx;
}

.shop_title {
  padding: 20rpx 20rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.shop_title text {
  font-size: 30rpx;
  color: #121314;
  font-weight: 600;
}

.cart-box {
  width: 100%;
  height: auto;
}

.cart-list {
  width: 812rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  transition: margin-left 0.2s ease-in-out;
}

.cart-list>.cl_left {
  width: 750rpx;
  box-sizing: border-box;
  padding: 20rpx 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cart-list .cart-thumb {
  width: 177rpx;
  height: 174rpx;
  margin-right: 0 20rpx;
  border: 0.5rpx solid #eee;
  border-radius: 15rpx;
}

.cart-right {
  width: 425rpx;
  /* height: 174rpx; */
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.shop {
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  margin-bottom: 20rpx;
}

.cart-list .cart-pro-name {
  width: 100%;
  /* height: 70%; */
  font-size: 28rpx;
  color: #121314;
  /* overflow: auto; */
  display: flex;
  flex-direction: column;
  margin:10rpx 0;
}

.cart-list .cart-pro-name>text {
  display: inline-block;
  font-size: 28rpx;
  color: #121314;
  /* overflow: hidden; */
}

.cart-list .cart-pro-name>text:nth-of-type(1) {
  /* height: 72rpx;
  overflow: auto; */
}

.cart-list .cart-pro-name>text:nth-of-type(2) {
  font-size: 22rpx;
  color: #949494;
  margin-top:5rpx;
}
.cart-list .cart-pro-name>text:nth-of-type(3) {
  font-size: 22rpx;
  color: red;
  margin-top:5rpx;
}

.cart-num {
  width: 100%;
  height:25%;
  margin-top:20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cart-list .cart-pro-price {
 width:60%;
 display: flex;
 justify-content:flex-start;
 align-items: center;
 white-space: nowrap;
 overflow: auto;
 position: relative;
}
.cart-list .cart-pro-price>text:nth-of-type(1){
  font-size: 26rpx;
  color: #f32c6e;
  margin-right: 10rpx;
}
.cart-list .cart-pro-price>text:nth-of-type(2){
  font-size: 12rpx;
  color: #ffffff;
  background: #f32c6e;
  padding: 5rpx;
  border-radius: 5rpx;
  margin-bottom: 15rpx;
}
.cart-list .cart-pro-price>text:nth-of-type(3){
  font-size:20rpx;
  color: #8d8d8d;
}
.cart-lable {
  display: block;
  width: 100%;
  height: 30rpx;
  font-size: 22rpx;
  color: #8d8d8d;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.cart-list .cart-count-box {
  width: 150rpx;
  height: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cart-list .cart-count-box text {
  display: inline-block;
  text-align: center;
  width: 33.33%;
  height: 100%;
  background: #eee;
  font-size: 22rpx;
  color: #121314;
  border-radius:5rpx;
  line-height: 30rpx;
}

.cart-list .cart-count-box .cart-count-down,
.cart-list .cart-count-box .cart-count-add {
  width: 26%;
  background: #d2d2d2;
  color: #8d8d8d;
}

.cart-count-down, .cart-count-add {
  width: 33.33%;
  height: 100%;
}

.cart-count-num {
  width: 33.33%;
  height: 100%;
}

.cart-del {
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 44rpx;
}

.delete-btn {
  width: 62rpx;
  line-height: 182rpx;
  text-align: center;
  background: #f32c6e;
  font-size: 24rpx;
  color: #fff;
}

.cart-footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
}

.cart-footer>view {
  height: 100%;
}

.allprice {
  width: 49%;
  height: 100%;
  display: flex;
  justify-content: flex-end;
  white-space: nowrap;
  overflow: auto;
  text-align: center;
  box-sizing: border-box;
  padding:0 10rpx;
}

.allprice text {
  text-align: center;
  line-height: 98rpx;
}

.allprice_text {
  color: #121314;
}

.cart-toatl-price {
  font-size: 36rpx;
  color: #f32c6e;
}

.order-icon {
  width: 25.35%;
  background: #f32c6e;
  line-height: 98rpx;
  text-align: center;
  color: #fff;
}
.order-icon>text{
  font-size: 30rpx;
}

.cart-no-data {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  /* position: absolute; */
}

.cart-no-data>image {
  width: 400rpx;
}

.cart-no-data>view {
  font-size: 30rpx;
  color: #8d8d8d;
}
