page {
  background: #f1f1f1;
}

.header_coupon {
  width: 100%;
  height: 93rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: #fff;
  box-sizing: border-box;
}

.nav {
  font-size: 24rpx;
  color: #121314;
  padding-bottom: 15rpx;
  border-bottom: 8rpx solid #fff;
}

.nav_active {
  border-bottom: 8rpx solid #f32c6e;
  color: #f32c6e;
}

.coupon_content {
  width: 100%;
}

.cc_item {
  width: 633rpx;
  height: 271rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  border-top: 20rpx solid #f32c6e;
  margin: 20rpx auto;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  display: none;
}

.cc_item_top {
  width: 100%;
  height: 78.5%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.cc_item_top>view {
  width: 30%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.cct_left>view {
  width: 100%;
  font-size: 31.5rpx;
  color: #121314;
  margin-bottom: 10rpx;
  text-align: center;
}

.cct_left>view>text {
  font-size: 60rpx;
  color: #121314;
}

.cct_left>.spq {
  background: #f32c6e;
  color: #fff;
  padding: 3rpx 20rpx;
  border-radius: 5rpx;
  margin-top: 10rpx;
}

.cc_item_top>.cct_center {
  width:30%;
  display: flex;
  flex-direction: column;
  justify-content:space-around;
  align-items: flex-start;
}

.cct_center>view {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
}

.cct_center>view>text {
  color: #f32c6e;
  font-weight: 600;
  text-align: center;
}

.cct_center>view>text:nth-of-type(1) {
  font-size: 36rpx;
  font-weight: 600;
}
