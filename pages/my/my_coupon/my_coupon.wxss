.cct_use {
  padding: 3rpx 20rpx;
  font-size: 22rpx;
  color: #fff;
  background: #f32c6e;
  border-radius: 10rpx;
}

.cc_item_bottom {
  width: 95%;
  height: 21.5%;
  margin: 0 auto;
  box-sizing: border-box;
  border-top: 0.5rpx dashed #f32c6e;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 20rpx;
}

.cc_item_bottom>text {
  display: block;
  width: 50%;
  white-space: nowrap;
  overflow: auto;
  font-size: 22rpx;
  color: #f32c6e;
}

.cc_item_bottom>text:nth-of-type(1) {
  white-space: nowrap;
  overflow: auto;
}

.cc_item_bottom>text:nth-of-type(2) {
  text-align: right;
}

