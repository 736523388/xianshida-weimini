page{
  background:#f1f1f1;
}
.footprint_date{
  font-size: 20rpx;
  color: #8d8d8d;
  padding: 12rpx 20rpx;
}

.cart-list {
  width: 750rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  transition: margin-left 0.2s ease-in-out;
}

.cart-list>.cl_left {
  width: 750rpx;
  box-sizing: border-box;
  padding: 20rpx 34rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 0.5rpx solid #f1f1f1;
}

.cart-list .cart-thumb {
  width: 177rpx;
  height: 174rpx;
  margin-right: 0 20rpx;
  border: 0.5rpx solid #eee;
  border-radius: 15rpx;
}

.cart-right {
  width:70%;
  /* height: 174rpx; */
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}

.shop {
  width: 100%;
  height: auto;
  background: #fff;
  margin-bottom: 20rpx;
}

.cart-list .cart-pro-name {
  width: 100%;
  /* height: 70%; */
  font-size: 28rpx;
  color: #121314;
  /* overflow: auto; */
  display: flex;
  flex-direction: column;
}

.cart-list .cart-pro-name>text {
  display: inline-block;
  font-size: 28rpx;
  color: #121314;
  overflow: hidden;
}

.cart-list .cart-pro-name>text:nth-of-type(1) {
  /* height: 72rpx; */
  overflow: auto;
}

.cart-list .cart-pro-name>text:nth-of-type(2) {
  font-size: 22rpx;
  color: #949494;
  margin-top: 10rpx;
}

.cart-num {
  width: 100%;
  height:25%;
  display: flex;
  justify-content: space-between;
  align-items: flex-end;
 margin: 10rpx 0;
}
.cart-num>image{
  width:30rpx;
  height:30rpx;
}

.cart-list .cart-pro-price {
 width:60%;
 display: flex;
 justify-content:flex-start;
 align-items: center;
 white-space: nowrap;
 overflow: auto;
 position: relative;
}
.cart-list .cart-pro-price>text:nth-of-type(1){
  font-size: 26rpx;
  color: #f32c6e;
  margin-right: 10rpx;
}
.cart-list .cart-pro-price>text:nth-of-type(2){
  font-size: 12rpx;
  color: #ffffff;
  background: #f32c6e;
  padding: 5rpx;
  border-radius: 5rpx;
  margin-bottom: 15rpx;
}
.cart-list .cart-pro-price>text:nth-of-type(3){
  font-size:20rpx;
  color: #8d8d8d;
}
.cart-lable {
  display: block;
  width: 100%;
  height: 30rpx;
  font-size: 22rpx;
  color: #8d8d8d;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.cart-list .cart-count-box {
  width: 150rpx;
  height: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.cart-list .cart-count-box text {
  display: inline-block;
  text-align: center;
  width: 33.33%;
  height: 100%;
  background: #eee;
  font-size: 22rpx;
  color: #121314;
  border-radius:5rpx;
  line-height: 30rpx;
}
