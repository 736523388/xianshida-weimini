page{
  background: #a20e46;
}
.made_box{
  width: 690rpx;
  margin:25rpx 30rpx;
  border-radius: 20rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  position: relative;
}
.made_box>view:nth-of-type(1){
  font-size: 33rpx;
  color: #393939;
  font-weight: 600;
  margin: 30rpx 0;
}
.made_box>view:nth-of-type(2){
 box-sizing: border-box;
 padding: 0 50rpx; 
}
.made_box>view:nth-of-type(2) view{
 font-size: 28rpx;
 color: #393939;
}
.made_box>view:nth-of-type(3){
 font-size:24rpx;
  color: #ff002a;
  margin: 80rpx 0 10rpx 0;
}
.f_service {
  width: 607rpx;
  height: 97rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom:100rpx;
  border-radius: 50rpx;
}

.f_service>image {
  width: 31rpx;
  height: 33rpx;
  margin-right: 10rpx;
}

.f_service>text {
  font-size: 28rpx;
  color: #fff;
}
.pig_logo{
  width: 118rpx;
  height:57rpx;
  position: absolute;
  right: 10rpx;
  bottom: 10rpx;
}