page{
  background:#f1f1f1;
}
.box{
  width: 100%;
}
.box>view{
 width: 100%;
 margin-top: 20rpx;
}
navigator{
  width: 100%;
  height:100rpx;
  display: flex;
 justify-content: space-between;
 align-items: center;
 background: #fff;
 box-sizing: border-box;
 padding:0 20rpx;
 margin-bottom: 2rpx;
}
navigator>text{
 font-size: 30rpx;
 color: #747272;
}
navigator>image{
  width:10rpx;
  height:19rpx;
}

