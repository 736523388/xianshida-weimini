/* pages/index/bargin/bargin.wxss */
page{
  background: linear-gradient(#ED2C6B,#DD2563,#af114d);
}
/* top */
.bargain-top{
  position: relative;
  width: 100vw;
  /* height: 890rpx; */
}
.bargain-top>image{
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.top-logo{
  padding-top: 32rpx;
  position: relative;
}
.top-logo>image{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}
.top-logo .wrap>view{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  position: relative;
  z-index: 1;
  align-items: center;
}
.top-logo .logo{
  width: 121rpx;
  height: 58rpx;
}
.top-logo .logo image{
  width: 100%;
  height: 100%;
}
.top-logo navigator{
  width: 133rpx;
  height: 39rpx;
  position: relative;
  /* border: 1px solid rgb(196, 192, 192); */
  border-radius: 22rpx;
  box-shadow: 0 0 4px #fff;
}
.top-logo navigator image{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: -1;
}
.top-logo navigator text{
  position: relative;
  font-size: 24rpx;
  color: #fff;
  left: 44rpx;
  top: 0rpx;
}
/* 用户信息 */
.top-information{
  text-align: center;
  position: relative;
}
.iNickname{
  padding-top: 46rpx;
  font-size: 24rpx;
  color: #8d8d8d;
}
.iHead{
  position: absolute;
  top: -34rpx;
  width: 74rpx;
  height: 74rpx;
  border: 2px solid #fff2fc;
  border-radius: 50%;
  left: 44%;
}
.iHead image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.iText{
  font-size: 30rpx;
  color: #000;
  width: 100%;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.iRule{
  position: absolute;
  top: 22rpx;
  right: 0;
  width: 118rpx;
  height: 34rpx;
  line-height: 32rpx;
  text-align: center;
  font-size: 24rpx;
  color: #8d8d8d;
  border: 1px solid #A0A0A0;
  border-radius: 22px;
}
/* 砍价的产品 */
.top-padding{
  padding: 0 20rpx 10px 20rpx;
  position: relative;
  background: #fff;
  border-top-left-radius: 14rpx;
  border-top-right-radius: 14rpx;
  margin-top: 32rpx;
}
.top-product{
  margin: 19rpx 0;
  padding: 28rpx 16rpx;
  background: #eee;
  border-radius: 6px;
}
.top-product .afleft{
  width: 170rpx;
  height: 162rpx;
}
.top-product .afleft image{
  width: 100%;
  height: 100%;
}
.top-product .afright{
  width: 438rpx;
}
.product-text1{
  font-size: 26rpx;
  color: #121314;
}
.product-text1,
.product-text2{
  width: 100%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.product-text2,
.product-text3{
  font-size: 22rpx;
  color: #949494;
}
.product-text3{
  margin: 8rpx 0;
}
.product-text3 text{
  font-size: 36rpx;
  color: #f32c6e;
}
.product-text4{
  color: #000;
}
.product-text4>view{
  width: 34rpx;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 22rpx;
  color: #fff;
  text-align: center;
  border-radius: 2px;
  background: #000;
}
.product-text4>text{
  font-size: 22rpx;
  color: #949494;
  margin-left: 4rpx;
}
.bargain-icon{
  position: absolute;
}
.bargain-icon1{
  left:-14rpx;
  top:22rpx;
  width: 76rpx;
}
.bargain-icon2{
  right:-18rpx;
  top:68rpx;
  width:36rpx;
}
.bargain-icon3{
  right:-18rpx;
  top:228rpx;
  width:36rpx;
}
/* 砍价的进度 */
.top-progress{
  padding: 22rpx 0;
  /* height: 90rpx; */
  position: relative;
}
.progress-past{
  position: absolute;
  top: 0;
  left: 0;
  /* width: auto; */
  height: 30rpx;
  line-height: 30rpx;
  border-radius: 22px;
  background: #ffb641;
}
.progress-past view{
  width: 110rpx;
  /* padding: 0 6rpx; */
  font-size: 18rpx;
  color: #f32c6e;
  text-align: center;
  font-weight: bold;
  position: relative;
}
.price-text::before{
  position: absolute;
  top: 100%;
  right: 12%;
  content: "";
  border: 8rpx solid transparent;
  border-top: 8rpx solid #ffb641 ;
}
.top-progress progress{
  border-radius: 22px;
  border: 1px solid #F32E6E;
  background: #fff;
  margin-top: 20rpx;
}
.progress-price{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  font-size: 22rpx;
  color: #8d8d8d;
}
.progress-price .original{
  text-decoration: line-through;
}
/* 分享按钮 */
.top-share{
  width: 90%;
  height: 70rpx;
  margin: 0 auto;
}
.top-share image{
  width: 100%;
  height: 100%;
}
/* 分享按钮 */
.top-share{
  animation: scale1 1.5s linear infinite;
  transition: all ease 0.8s;
  /* box-shadow: 0 0 6px #333; */
}
@keyframes scale1{
  0%{
                transform: scale(1);  /*开始为原始大小*/
            }
            25%{
                transform: scale(1.05); /*放大1.1倍*/
            }
            50%{
                transform: scale(1);
            }
            75%{
                transform: scale(1.05);
            }

}
/* 斧头帮 */
.top-list{
  padding: 0 20rpx 20rpx 20rpx;
  background: #FEDEDE;
  border-bottom-left-radius: 14rpx;
  border-bottom-right-radius: 14rpx;
  overflow: hidden;
}
.list-title{
  font-size: 26rpx;
  color: #121314;
  text-align: center;
  /* padding-top: 52rpx; */
}
.list-title image{
  width: 100%;
  height: 70rpx;
}
.list-mian{
  /* margin-top: 12rpx; */
}
.bList-move{
