.cont-right{
  text-align: right;
}
.cont-right image{
  width: 54rpx;
  height: 54rpx;
}
.cont-right view{
  font-size: 26rpx;
  color: #121314;
}
.cont-right view text{
  color: #f32e6e;
}
/* 猜你喜欢 */
