  /* border-top-color: #F8F8F7;
  border-top-width: 2rpx;
  border-top-style: solid; */
}

.nominate_div image {
  width: 356rpx;
  height: 199rpx;
}

.limit-discount-content {
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}

.limit-discount-div {
  width: 353rpx;
  font-size: 0;
  margin-bottom: 8rpx;
}

