/* pages/index/productContent/productContent.wxss */
page{
  background: #f1f1f1;
}
/* banner */
.pCont-ban{
  width: 100%;
  height: 700rpx;
  background: #fff;
}
.pCont-ban image{
  width: 100%;
  height: 100%;
}
/* 倒计时 */
.secondCont-time{
  height: 88rpx;
  position: relative;
}
.secondCont-time>image{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
}
.secondCont-time .wrap{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.secondCont-time .wrap .left text,
.secondCont-time .wrap .left view,
.right-cont view{
  display: inline-block;
  vertical-align: middle;
}
.secondCont-time .wrap .left{
  line-height: 88rpx;
  font-size: 36rpx;
  color: #fff;
}
.secondCont-time .wrap .left view{
  width: 68rpx;
  height: 24rpx;
  line-height: 22rpx;
  text-align: center;
  font-size: 18rpx;
  color: #f32e6e;
  border-radius: 22px;
  background: #fff;
  margin-left: 32rpx;

}
.secondCont-time .wrap .right{
  padding-top: 14rpx;
  font-size: 22rpx;
  color: #fff;
  text-align: center;
}
.secondCont-time .wrap .right-cont view{
  width: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  font-size: 24rpx;
  background: rgba(0,0,0, 0.5);
  border-radius: 50%;
}
/* 文字 */
/* 价格 */
.pCont-price .wrap{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.pCont-price .left{
  width: 88%;
  padding: 20rpx 0;
}
.price1-text1{
  font-size: 38rpx;
  color: #f32c6e;
  margin-right: 20rpx;
}
.price1-text2{
  height: 22rpx;
  line-height: 22rpx;
  padding: 0 6rpx;
  font-size: 18rpx;
  color: #fff;
  border-radius: 2px;
  background: #f32c6e;
  vertical-align: top;
  margin-bottom: 12rpx;
}
.price1-text3{
  font-size: 28rpx;
  color: #8d8d8d;
  margin-right: 8rpx;
}
.price1-text4{
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 6rpx;
  font-size: 24rpx;
  color: #121314;
  border-radius: 2px;
  background: #e5e5e5;
}
.price2{
  font-size: 28rpx;
  color: #121314;
  font-weight: 600;
  /* margin-bottom: 10rpx; */
}
.price3{
  font-size: 23rpx;
  color: #949494;
}
.pCont-price .right{
  width: 88rpx;
  height: 113rpx;
  position: fixed;
  right: 20rpx;
  top: 67%;
}
.pCont-price .right image{
  width: 100%;
  height: 100%;
}
/* 优惠券 */
.pCont-discount .wrap{
  height: 60rpx;
  line-height: 60rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.pCont-discount .wrap .left{
  font-size: 28rpx;
  color: #44464e;
  font-weight: 600;
}
.pCont-discount .wrap .right{
  font-size: 24rpx;
  color: #8d8d8d;
}
.pCont-discount .wrap .right image{
  width: 12rpx;
  height: 20rpx;
  margin-left: 12rpx;
}
/* 开通会员 */
.pCont-opening .wrap>view{
  padding: 28rpx  102rpx 28rpx 0;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.pCont-opening .wrap .left{
  font-size: 26rpx;
  color: #474950;
}
.pCont-opening .wrap .left image{
  width: 30rpx;
  height: 30rpx;
  margin: 0 18rpx 0 12rpx;
}
.pCont-opening .wrap .right view{
  width: 130rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  font-size: 26rpx;
  color: #fff;
  border-radius: 4px;
  background: #f32c6e;
}
/* 服务 */
.service1{
  padding: 20rpx 0;
  font-size: 26rpx;
  border-bottom: 1px solid #e5e5e5;
}
.service1 .wrap>view:first-of-type{
  width: 10%;
  color: #121314;
}
.service1 .wrap .project{
  width: 90%;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
}
.service1 .wrap .project .spot{
  font-size: 16rpx;
  color: #f32c6e;
  margin-right: 6rpx;
}
.service1 .wrap .project .text{
  color: #8d8d8d;
}
/* 图标 */
.service2{
  padding: 20rpx 0;
}
.service2 .wrap{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
  font-size: 26rpx;
  color: #333538;
}
.service2 .wrap view image{
  width: 45rpx;
  height: 48rpx;
  margin-right: 18rpx;
}
/* 相关推荐 */
.recommend-title{
  height: 80rpx;
  line-height: 80rpx;
  font-size: 34rpx;
  color: #333538;
}
/* 推荐的产品 */
.recommend-cont{
  width: 345rpx;
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  background: #fff;
  border-radius: 8px;
}
.recommend-cont:nth-child(2n){
  margin-right: 0;
}
.recommend-cont .img{
  width: 100%;
  height: 260rpx;
  border-bottom: 1px solid #eee;
  box-sizing: border-box;
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
}
.recommend-cont .img image{
  border-top-left-radius: 8px;
  border-top-right-radius: 8px;
  width: 100%;
  height: 100%;
}
.recommend-text{
  padding: 16rpx 18rpx;
}
.text-title{
  font-size: 28rpx;
  color: #121314;
}
.text-font{
  font-size: 26rpx;
  color: #949494;
  margin-bottom: 22rpx;
}
.text-price .price1{
  font-size: 28rpx;
  color: #f32c6e;
  margin-right: 12rpx;
}
.text-price .price2{
  height:22rpx;
  line-height:22rpx;
  padding:0 6rpx;
  font-size:18rpx;
  color:#fff;
  border-radius:2px;
  background:#f32c6e;
  margin-bottom:12rpx;
}
.text-price .price3{
  font-size:22rpx;
  color:#8d8d8d;
  margin-right:8rpx;

}
/* 商品详情 */
/* title */
.details-title>.wrap{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
}
.details-title{
  background: #fff;
  font-size: 32rpx;
  color: #aaa;
  border-bottom: 1px solid #f1f1f1;
}
.details-title .left{
  padding: 35rpx 0;
  position: relative;
}
.details-title #details{
  position: relative;
  color: #f32e6e;
}
.details-title #details::before{
  position: absolute;
  content: "";
  top: 100%;
  left: 24rpx;
  width: 80rpx;
  height: 2px;
  background: #f32e6e;
}
.details-title .right{
  padding-top: 35rpx;
}
.details-title .right view:last-child{
  font-size: 20rpx;
}
.details-title .right view:last-child text{
  color: #f32e6e;
}
.details1 image{
  display: block;
  width: 100%;
}
.pCont-prohibit{
  margin-top: 10rpx;
  background: #fff;
}
.pCont-prohibit .wrap>view{
  padding: 52rpx 8rpx 0 8rpx;
}
.pCont-prohibit .wrap>view>view{
  margin-bottom: 42rpx;
}
.prihibit-title{
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.prihibit-text{
  font-size: 28rpx;
  color: #767676;
}
.prihibit-bottom{
  font-size: 28rpx;
  color: #8d8d8d;
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  background: #f1f1f1;
  border-top: 1px solid #e7e7e7; 
  margin-bottom: 96rpx;
}
/*  tabar */
.pCont-tabar{
  width: 100%;
  height: 96rpx;
  background: #fff;
  border-top: 1px solid #e7e7e7;
  position: fixed;
  left: 0;
  bottom: 0;
}
.pCont-tabar>view{
  width: 50%;
}
.pCont-tabar>.afleft{
  padding-top: 13rpx;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
}
.pCont-tabar>.afleft>view{
  width: 74rpx;
  text-align: center;
  font-size: 24rpx;
  color: #575757;
}
.pCont-tabar>.afleft>view image{
  display: block;
  width: 44rpx;
  height: 44rpx;
  margin: 0 auto;
}
.tabar3{
  position: relative;
}
.tabar3-num{
  position: absolute;
  right: -18rpx;
  top: 0;
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  background: #f32e6e;
  border-radius: 50%;
}
.pCont-tabar>.afright>view{
  width: 50%;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  font-size: 26rpx;
  color: #fff;
}
.pCont-tabar>.afright>view.afleft{
  background: #f32e6e;
}
.pCont-tabar>.afright>view.afright{
  background: #d72861;
}
/* 评价 */
/* tab */
.evaluate-tab{
  padding: 30rpx 0;
  border-bottom: 1px solid #f1f1f1;
  background: #fff;
}
.evaluate-tab .wrap view{
  width: 135rpx;
  height: 46rpx;
  line-height: 44rpx;
  text-align: center;
  font-size: 22rpx;
  color: #7e7e7e;
  background: #e5e5e5;
  border-radius: 4px;
  margin-right: 12rpx;
}
.evaluate-tab .wrap view.active{
  color: #fff;
  background: #f32e6e;
}
/* 评价列表 */
.list-cont{
  padding: 24rpx 0;
  border-bottom: 1px solid #ececec;
  background: #fff;
}
/* 昵称 */
.list-nickname{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
  font-size: 22rpx;
}
.list-nickname .left .img{
  width: 38rpx;
  height: 38rpx;
  border: 1px solid #ddd;
  border-radius: 50%;
  margin-right: 16rpx;
}
.list-nickname .left image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.list-nickname .left text{
  color: #121314;
}
.list-nickname .right{
  color: #8d8d8d;
}
.list-text{
  font-size: 24rpx;
  color: #636363;
  margin-bottom: 16rpx;
}
.list-img image{
  width: 128rpx;
  height: 146rpx;
  margin:  0 16rpx 18rpx 0;
}
.list-img image:nth-child(5n){
  margin-right: 0;
}
.pCont-price,
.pCont-discount,
.pCont-opening,
.pCont-service,
.recommend-title{
  background: #fff;
  margin-bottom: 20rpx;
}
/* 选择规格 */
.specifications-box{
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0,0,0, 0.6);
  z-index: 20;
}
.sMain{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 855rpx;
  background: #fff;
  transition: all 2s ease;
}
.cont-img{
  height: 146rpx;
  border-bottom: 1px solid #e5e5e5;
}
.cont-img .wrap{
   display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.cont-img .left{
  margin-top: -48rpx;
}
.cont-img .left .img{
  width: 180rpx;
  height: 173rpx;
  border-radius: 6px;
  box-shadow: 1px 1px 6px #44464e;
  margin-right: 20rpx;
}
.cont-img .left .img image{
  width: 100%;
}
.cont-img .left .img-price{
  margin-top: 44rpx;
}
.cont-img .left .img-price view{
  font-size: 38rpx;
  color: #f32c6e;
  line-height: 6rpx;
}
.cont-img .left .img-price text{
  font-size: 26rpx;
  color: #8d8d8d;
}
.cont-img .right view{
  padding-top: 10rpx;
  font-size: 36rpx;
  color: #464647;
}
/* 规格  数量  温馨提示 */
/* 规格 */
.cont-num{
  height: 600rpx;
  overflow: auto;
}
.specifications{
  overflow: hidden;
  padding-top: 18rpx;
  border-bottom: 1px solid #e5e5e5;
}
.cont-title{
  font-size: 26rpx;
  color: #000;
  margin-top: 4rpx;
}
.specifications .cont-title{
  margin-bottom: 14rpx;
}
.cont-option{
  display: flex;
  flex-flow: row wrap;
  justify-content: flex-start;
}
.cont-option view{
  height: 34rpx;
  line-height: 34rpx;
  padding: 0 36rpx;
  font-size: 24rpx;
  color: #000;
  border: 1px solid #e5e5e5;
  border-radius: 4px;
  margin: 0 14rpx 14rpx 0;
}
.cont-option view.active,
.btn-box view.active{
  color: #fff;
  border-color: #f32e6e;
  background: #f32e6e;
}
/* 数量 */
.num-box{
  padding: 20rpx 0;
   border-bottom: 1px solid #e5e5e5;
}
.num-box .afright view{
  font-size: 24rpx;
  height: 34rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 4px;
}
.plus,
.reduce{
  padding: 0 16rpx;
}
.reduce{
  color: #96999c;
  background: #e5e5e5;
  margin-right: 10rpx;
}
.plus{
  color: #36383a;
  background: #b5b5b5;
}
.num-box .afright .num{
  padding: 0 34rpx;
  color: #36383a;
  background: #e5e5e5;
  margin-right: 10rpx;
}
/* 温馨提示 */
.tips view{
  height: 90rpx;
  line-height: 90rpx;
  font-size: 26rpx;
  color: #474950;
  background: #ffedd2;
  border: 1px solid #f29b76;
  margin: 80rpx auto;
}
.tips view .red{
  color: #ff0000;
  margin: 0 20rpx;
}
/* 购买按钮 */
.btn-box{
  width: 100vw;
  position: fixed;
  left: 0;
  bottom: 26rpx;
}
.btn-box .btn{
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
  font-size: 28rpx;
  color: #a9a8a8;
  border-radius: 6px;
  background: #dcdcdc;
}
/* 分享弹出层 */
.productContent-share,
.imgShare-box{
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0,0,0, 0.5)
}
.share-box{
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100vw;
  height: 200rpx;
  background: #fff;
}
.share-cont{
  position: relative;
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-around;
  padding: 32rpx 0;
}
.share-icon{
  width: 168rpx;
  text-align: center;
}
.share-img{
  display: block;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 0 auto 10rpx auto;
}
.share-img image,
.share-close image{
  width: 100%;
  height: 100%;
}
.share-text{
  /* line-height: 0; */
  font-size: 24rpx;
  color: #121314;
}
.share-close{
  width: 20rpx;
  height: 20rpx;
  position: absolute;
  top: -12rpx;
  right: 10rpx;
}
/* 分享图片 */
.imgShare-cont{
  width: 618rpx;
  height: 970rpx;
  margin: 120rpx auto 0 auto;
  border-radius: 8px;
  background: #fff;
}




.price1 view,
.pCont-discount .wrap .right image,
.pCont-discount .wrap .right text,
.pCont-opening .wrap .left text,
.pCont-opening .wrap .left image,
.service1 .wrap view,
.service1 .wrap text,
.service2 .wrap view image,
.service2 .wrap view text,
.text-price view,
.evaluate-tab .wrap view,
.list-nickname .left .img,
.list-nickname .left text,
.cont-img .left .img,
.cont-img .left .img-price,
.num-box .afright view{
  display: inline-block;
  vertical-align: middle;
}

