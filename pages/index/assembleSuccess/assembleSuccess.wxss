/* pages/index/assembleSuccess/assembleSuccess.wxss */
.top-cont1 .list-cont{
  padding: 30rpx 0;
}
.top-cont1 .list-img{
  width: 177rpx;
  height: 177rpx;
  border-radius: 6px;
  margin-right: 22rpx;
  margin-left: 16rpx;
}
.top-cont1 .list-title{
  font-size: 26rpx;
  color: #121314;
  padding-top: 10rpx;
}
.top-cont1 .list-title,
.top-cont1 .list-text1{
  width: 352rpx;
  /* overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; */
}
.top-cont1 .list-text1{
  font-size: 24rpx;
  color: #949494;
}
.top-cont1 .list-text2{
  font-size: 22rpx;
  color: #bdbdbd;
  margin-bottom: 4rpx;
}
.top-cont1 .list-num{
  width: 446rpx;
}
.top-cont1 .list-people{
  width: 46rpx;
  height: 18rpx;
  line-height: 18rpx;
  text-align: center;
  font-size: 14rpx;
  color: #fff;
  background: #f32e6e;
  border-radius: 2px;
  margin-bottom: 12rpx;
}
.top-cont1 .list-price1{
  font-size: 26rpx;
  color: #f32e6e;
  margin-right: 22rpx;
}
.top-cont1 .list-price2{
  font-size: 22rpx;
  color: #8d8d8d;
}
.top-cont1 .list-btn{
  width: 110rpx;
  height: 36rpx;
  line-height: 34rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  background: #f32e6e;
  border-radius: 4px;
  margin-top: 14rpx;
}
/* 税费补贴 */
.top-cont2{
  height: 50rpx;
  line-height: 42rpx;
  font-size: 22rpx;
  color: #8d8d8d;
  background: #f1f1f1;
}
.top-cont2>.wrap{
  display: flex;
  flex-flow: row nowrap;
  justify-content: space-between;
}
.tCont2-content image{
  width: 25rpx;
  height: 25rpx;
  margin-right: 10rpx;
}
/* 查看我的拼团 */
.top-cont3{
  text-align: center;
  padding: 14rpx 0 36rpx 0;
}
.top-cont3 .wrap>image{
  width: 192rpx;
  height: 146rpx;
}
.tCont3-headImg{
  margin: 30rpx 0 22rpx 0;
}
.tCont3-headImg>view{
  width: 65rpx;
  height: 65rpx;
  border-radius: 50%;
}
.tCont3-headImg>view image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.headImg1{
  position: relative;
  margin-right: 26rpx;
}
.headImg1 view{
  position: absolute;
  top: 0;
  right: -26rpx;
  width: 52rpx;
  height: 24rpx;
  line-height: 22rpx;
  text-align: center;
  font-size: 18rpx;
  color: #fff;
  border-radius: 22px;
  background: #f32e6e;
  border: 1px solid #fff;
}
.tCont3-btn{
  width: 498rpx;
  height: 42rpx;
  line-height: 42rpx;
  text-align: center;
  color: #f32e6e;
  font-size: 24rpx;
  border-radius: 22px;
  border: 1px solid #f32e6e;
  margin: 0 auto;
}
/* 拼团须知 */
.top-cont4{
  border-top: 1px solid #f1f1f1;
  border-bottom: 10rpx solid #f1f1f1;
}
.top-cont4>.wrap{
  height: 50rpx;
  line-height: 50rpx;
   font-size: 24rpx;
}
.top-cont4 .afleft{
  color: #121314;
}
.top-cont4 .afright{
  color: #8d8d8d;
}
/* 猜你喜欢 */
.bottom-title{
  font-size: 24rpx;
  color: #121314;
  padding-top: 14rpx;
}
.aSuccess-bottom .list-cont{
  width: 224rpx;
  margin: 18rpx 18rpx 0 0;
  border-radius: 6px;
  box-shadow: 0 0 4px rgb(161, 160, 160);
}
.aSuccess-bottom .list-cont:nth-child(3n){
  margin-right: 0;
}
.aSuccess-bottom .list-img{
  width: 100%;
  height: 206rpx;
  border-bottom: 1px solid #f1f1f1;
  position: relative;
  border-top-left-radius: 6px;
  border-top-right-radius: 6px;
}
.aSuccess-bottom .list-img image{
   border-top-left-radius: 6px;
  border-top-right-radius: 6px;
}
.aSuccess-bottom .list-img view{
  position: absolute;
  right: 5rpx;
  bottom: 6rpx;
  height: 26rpx;
  line-height: 26rpx;
  text-align: center;
  font-size: 16rpx;
  color: #f32c6e;
  padding: 0 8rpx;
  border-radius: 22px;
  background: #fde5ed;
}
.aSuccess-bottom .list-text{
  padding: 10rpx;
}
.aSuccess-bottom .list-title{
  font-size: 23rpx;
  color: #121314;
}
.aSuccess-bottom .list-price1{
  font-size: 26rpx;
  color: #f32e6e;
  margin-top: 6rpx;
}
.aSuccess-bottom .list-price1 text{
  font-size: 22rpx;
}
.aSuccess-bottom .list-price2{
  font-size: 20rpx;
  color: #8d8d8d;
}
.aSuccess-bottom .list-link{
  width: 100%;
  height: 35rpx;
  line-height: 35rpx;
  text-align: center;
  font-size: 20rpx;
  color: #fff;
  border-radius: 4px;
  background: #f32e6e;
  margin-top: 12rpx;
}



.top-cont1 .list-img,
.top-cont1 .list-text,
.top-cont1 .list-people,
.top-cont1 .list-price1,
.top-cont1 .list-price2,
.tCont2-content image,
.tCont2-content text,
.headImg1,
.headImg2{
  display: inline-block;
  vertical-align: middle;
}
.list-img image,
.list-img image{
  width: 100%;
  height: 100%;
}